@using CornerFlag.Data.Models.Entities
@model IQueryable<Club>
@{    
    ViewBag.Title = "All";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Clubs</h1>
<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Year Founded</th>
                <th>Competitions</th>
                <th>Teams</th>
                <th>Detail</th>
            </tr>
        </thead>
        <tbody class="alternate-color-table">
            @foreach (var club in Model)
            {
                Html.RenderPartial("_ClubInfoTableRow", club);
            }
        </tbody>

    </table>
</div>